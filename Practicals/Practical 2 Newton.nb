Notebook[{
Cell[TextData[{
 "                                                                            \
                               ",
 StyleBox["                                                          \
Practical-2\n                                                                 \
                             Newton Raphson Method ", "Subtitle"],
 "\n\n",
 StyleBox["(1) To find a smallest positive root of the function ",
  FontFamily->"Times New Roman",
  FontSize->20],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     SuperscriptBox["x", "3"], "-", 
     RowBox[{"5", "x"}], "+", "1", " "}]}], TraditionalForm]],
  FontFamily->"Times New Roman",
  FontSize->20],
 StyleBox["perform 5 iterations of the Newton Raphson Method ",
  FontFamily->"Times New Roman",
  FontSize->20]
}], "Text",
 CellChangeTimes->{{3.745024032059255*^9, 3.74502409051538*^9}, {
  3.7450241240796022`*^9, 3.7450243144956245`*^9}, {3.745024395734167*^9, 
  3.7450245203655615`*^9}, {3.822530669923933*^9, 3.8225306836521*^9}, {
  3.838869086817466*^9, 3.8388690872081976`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{
    RowBox[{"NewtonRaphson", "[", 
     RowBox[{"x0_", ",", "n_", ",", "f_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xk1", ",", 
        RowBox[{"xk", "=", 
         RowBox[{"N", "[", "x0", "]"}]}]}], "}"}], ",", 
      RowBox[{
       RowBox[{"k", "=", "0"}], ";", " ", 
       RowBox[{"Output", "=", " ", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"k", ",", "x0", ",", 
           RowBox[{"f", "[", "x0", "]"}]}], "}"}], "}"}]}], ";", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"k", "<", "n"}], ",", 
         RowBox[{
          RowBox[{"fPrimexk", "=", 
           RowBox[{
            RowBox[{"f", "'"}], "[", "xk", "]"}]}], ";", " ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"fPrimexk", "\[Equal]", "0"}], ",", 
            RowBox[{
             RowBox[{"Print", "[", 
              RowBox[{
              "\"\<The derivative of function at \>\"", ",", "k", ",", 
               "\"\<th iteration is zero,we can not proceed further with the \
iterative scheme\>\""}], "]"}], ";", 
             RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
          RowBox[{"xk1", "=", 
           RowBox[{"xk", "-", 
            RowBox[{
             RowBox[{"f", "[", "xk", "]"}], "/", "fPrimexk"}]}]}], ";", 
          RowBox[{"xk", "=", "xk1"}], ";", 
          RowBox[{"k", "++"}], ";", 
          RowBox[{"Output", "=", 
           RowBox[{"Append", "[", 
            RowBox[{"Output", ",", 
             RowBox[{"{", 
              RowBox[{"k", ",", "xk", ",", 
               RowBox[{"f", "[", "xk", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
        "]"}], ";", 
       RowBox[{"Print", "[", 
        RowBox[{"NumberForm", "[", 
         RowBox[{
          RowBox[{"TableForm", "[", 
           RowBox[{"Output", ",", 
            RowBox[{"TableHeadings", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"None", ",", 
               RowBox[{"{", 
                RowBox[{
                "\"\<k\>\"", ",", "\"\<xk\>\"", ",", "\"\<f[xk]\>\""}], 
                "}"}]}], "}"}]}]}], "]"}], ",", "10"}], "]"}], "]"}], ";", 
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<Root after \>\"", ",", "n", ",", "\"\< iterations xk = \>\"", 
         ",", 
         RowBox[{"NumberForm", "[", 
          RowBox[{"xk", ",", "10"}], "]"}]}], "]"}], ";", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Function value at approximated root,f[xk] = \>\"", ",", 
         RowBox[{"NumberForm", "[", 
          RowBox[{
           RowBox[{"f", "[", "xk", "]"}], ",", "10"}], "]"}]}], "]"}], 
       ";"}]}], "]"}]}], ";"}],
  FontFamily->"Courier New",
  FontSize->18], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{
    RowBox[{"f", "[", "x_", "]"}], " ", ":=", 
    RowBox[{
     SuperscriptBox["x", "3"], "-", 
     RowBox[{"5", "x"}], "+", "1"}]}], ";"}],
  FontFamily->"Courier New",
  FontSize->18], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"f", "[", "x", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "10"}], ",", "10"}], "}"}]}], "]"}],
  FontFamily->"Courier New",
